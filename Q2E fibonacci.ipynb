    "def fibonacci(n):\n",
    "    count = 0\n",
    "    n1 = 0\n",
    "    n2 = 1\n",
    "    while count < n:\n",
    "        print(n1)\n",
    "        new_term = n1+n2\n",
    "        n1 = n2\n",
    "        n2 = new_term\n",
    "        count += 1\n",
    "n = int(input(\"How many terms of the fibonacci series?\"))\n",
    "fibonacci(n)"
   ]
